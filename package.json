    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "googleapis": "^140.0.1",
    "image-thumbnail": "^1.0.10",
    "jsonwebtoken": "^9.0.2",
    "mime-message": "^0.1.3",
    "mime-types": "^2.1.27",
